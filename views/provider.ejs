<table>
    <thead>
        <tr>
            <th>Provider Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Specialty</th>
            <th>Email</th>
            <th>Phone Number</th>
            <th>Member Since</th>
        </tr>
    </thead>
    <tbody>
        <% results.forEach(row => { %>
            <tr>
                <td><%= row.provider_id %></td>
                <td><%= row.first_name %></td>
                <td><%= row.last_name %></td>
                <td><%= row.provider_specialty %></td>
                <td><%= row.email_address %></td>
                <td><%= row.phone_number %></td>
                <td><%= new Date(row.date_joined).toISOString().split('T')[0] %></td> 
            </tr> 
        <% }); %>
    </tbody>
</table>